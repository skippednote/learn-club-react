{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "emotion": "^9.1.3",
    "history": "^4.7.2",
    "json-api-normalizer": "^0.4.10",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-emotion": "^9.1.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "^1.1.4",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "npm-run-all --parallel test:unit test:e2e",
    "test:unit": "react-scripts test --env=jsdom",
    "test:e2e": "npm-run-all --parallel --race start cypress:run",
    "test:coverage": "npm run test:unit -- --coverage && codecov",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "eject": "react-scripts eject",
    "flow": "flow",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "@storybook/addon-actions": "^3.4.3",
    "@storybook/addon-links": "^3.4.3",
    "@storybook/addons": "^3.4.3",
    "@storybook/react": "^3.4.3",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "codecov": "^3.0.2",
    "cypress": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint-plugin-cypress": "^2.0.1",
    "flow-bin": "^0.72.0",
    "husky": "^0.14.3",
    "jest-emotion": "^9.1.3",
    "lint-staged": "^7.1.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.12.1",
    "redux-devtools": "^3.4.1",
    "redux-mock-store": "^1.5.1",
    "serve": "^6.5.6"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write --single-quote --print-width 80",
      "git add"
    ]
  }
}
